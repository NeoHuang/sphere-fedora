{
    "id": "sphere-fedora",
    "name": "Sphere.IO fedora example web store",
    "source": "git://github.com/commercetools/sphere-fedora.git",
    "description": "Get started with Sphere.IO by building your own web store based on our fedora example.",
    "doc-url": "https://github.com/commercetools/sphere-fedora/blob/master/README.md",    
    "icon" : "http://commercetools.de/assets/img/ct_logo_farbe.jpg", 
    "disabled": false,
    "supported-by" : "commercetools", 
    "order" : 1,

    "runtime": {
        "type": "play2",
        "app-parameters" : { 
            "runtime.java_version": "1.7"
        },
        "app-variables" : { 
            "proxyBuffering" : false
        }
    },
    "databases": [],
    "build-with-jenkins": {
        "template": {
            "type": "https://raw.github.com/commercetools/sphere-fedora/master/deploy/cloudbees/config.xml"
        }
    }
}
